                                                        <Expander Header="{DynamicResource Cog_Individuation_Theme}">
                                                            <StackPanel Margin="5">
                                                                <Grid>
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition/>
                                                                        <ColumnDefinition/>
                                                                        <ColumnDefinition/>
                                                                    </Grid.ColumnDefinitions>
