<Window x:Class="KitX.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KitX"
        xmlns:controls="clr-namespace:KitX.Controls"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:popc="clr-namespace:PopEye.WPF.Controls;assembly=PopEye.WPF"
        xmlns:popcp="clr-namespace:PopEye.WPF.Controls.Panels;assembly=PopEye.WPF"
        xmlns:cef="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        x:Name="mainWindow" Icon="../Source/KitX.png"
        FlowDirection="LeftToRight"
        Title="KitX" MinHeight="620" MinWidth="980" Height="768" Width="1080"
        Background="{DynamicResource MaterialDesignBackground}" SnapsToDevicePixels="True">

    <!--#region 资源定义 -->
    <Window.Resources>

        <sys:String x:Key="KitX-Version">v2.0.4</sys:String>

        <!--#region 详细信息资源绑定 -->
        <sys:String x:Key="ToolName">ToolName</sys:String>
        <sys:String x:Key="Publisher">Catrol</sys:String>
        <sys:String x:Key="InstalledVersion">Alpha</sys:String>
        <sys:String x:Key="Discription">描述</sys:String>
        <sys:String x:Key="HelpLink">https://docs.catrol.cn</sys:String>
        <sys:String x:Key="HostLink">https://blog.catrol.cn</sys:String>
        <sys:String x:Key="LangProvided">zh-CN</sys:String>
        <BitmapImage x:Key="Icon" UriSource="../Source/KitX.png"/>
        <sys:String x:Key="Version">Alpha</sys:String>
        <!--#endregion-->

        <!--#region 账户信息资源绑定 -->
        <sys:String x:Key="UserName">UserName</sys:String>
        <sys:String x:Key="UserMail">UserMail@catrol.cn</sys:String>
        <sys:DateTime x:Key="UserBirthday">1949-10-01 14:00:00</sys:DateTime>
        <Visibility x:Key="UserSigned">Hidden</Visibility>
        <Visibility x:Key="YUserIcon">Visible</Visibility>
        <BitmapImage x:Key="UserHeader" UriSource="../Source/KitX.png"/>
        <!--#endregion-->

        <!--#region 转换器资源 -->
        <local:EditMenuEnable x:Key="EditMenuEnableConverter"/>
        <local:GetOpenFolder x:Key="GetOpenFolder"/>
        <local:GetAccViewMenuEnable x:Key="GetAccMenuEnable"/>
        <local:GetAccInfoViewMenuEnable x:Key="GetAccInfoMenuEnable"/>
        <local:GetHalfWidth x:Key="GetHalfWidth"/>
        <local:FlowDirConverter x:Key="FlowDir_Converter"/>
        <!--#endregion-->

        <!--#region 标题动画资源 -->
        <Storyboard x:Key="TitleEffect" AutoReverse="True" RepeatBehavior="Forever">
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="TitleBox">
                <EasingColorKeyFrame KeyTime="0:0:1" Value="#FF5E8BA6">
                    <EasingColorKeyFrame.EasingFunction>
                        <SineEase EasingMode="EaseInOut"/>
                    </EasingColorKeyFrame.EasingFunction>
                </EasingColorKeyFrame>
            </ColorAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Effect).(DropShadowEffect.BlurRadius)" Storyboard.TargetName="TitleBox">
                <EasingDoubleKeyFrame KeyTime="0:0:1" Value="10"/>
            </DoubleAnimationUsingKeyFrames>
            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Effect).(DropShadowEffect.Color)" Storyboard.TargetName="TitleBox">
                <EasingColorKeyFrame KeyTime="0:0:1" Value="#FF009FFF"/>
            </ColorAnimationUsingKeyFrames>
        </Storyboard>
        <!--#endregion-->

        <!--#region 动画信息资源绑定 -->
        
        <Storyboard x:Key="SignBig">
            <DoubleAnimation Duration="00:00:0.5" From="310" To="330" Storyboard.TargetName="SignInWin" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="470" To="490" Storyboard.TargetName="SignInWin" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="310" To="330" Storyboard.TargetName="SignUpWin" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="470" To="490" Storyboard.TargetName="SignUpWin" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>
        
        <Storyboard x:Key="SignSmall">
            <DoubleAnimation Duration="00:00:0.5" From="330" To="310" Storyboard.TargetName="SignInWin" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="490" To="470" Storyboard.TargetName="SignInWin" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="330" To="310" Storyboard.TargetName="SignUpWin" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="490" To="470" Storyboard.TargetName="SignUpWin" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>
        
        <Storyboard x:Key="SignUpInfoBig">
            <DoubleAnimation Duration="00:00:0.5" From="250" To="260" Storyboard.TargetName="SignUpInfoBox" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="300" To="340" Storyboard.TargetName="SignUpInfoBox" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <Storyboard x:Key="SignUpInfoSmall">
            <DoubleAnimation Duration="00:00:0.5" From="260" To="250" Storyboard.TargetName="SignUpInfoBox" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="340" To="300" Storyboard.TargetName="SignUpInfoBox" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <Storyboard x:Key="UserCardBig">
            <DoubleAnimation Duration="00:00:0.5" From="220" To="240" Storyboard.TargetName="UserCard" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="330" To="360" Storyboard.TargetName="UserCard" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <Storyboard x:Key="UserCardSmall">
            <DoubleAnimation Duration="00:00:0.5" From="240" To="220" Storyboard.TargetName="UserCard" Storyboard.TargetProperty="Width">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
            <DoubleAnimation Duration="00:00:0.5" From="360" To="330" Storyboard.TargetName="UserCard" Storyboard.TargetProperty="Height">
                <DoubleAnimation.EasingFunction>
                    <CubicEase EasingMode="EaseOut"/>
                </DoubleAnimation.EasingFunction>
            </DoubleAnimation>
        </Storyboard>

        <!--#endregion-->        

    </Window.Resources>
    <!--#endregion-->
    
    <!--#region 自定义窗体设定 -->
    <WindowChrome.WindowChrome>
        <WindowChrome GlassFrameThickness="0,30,0,0" CaptionHeight="0" NonClientFrameEdges="Left,Bottom,Right" ResizeBorderThickness="10"/>
    </WindowChrome.WindowChrome>
    <!--#endregion-->

    <!--#region 内容 -->
    <Window.Template>
        <ControlTemplate TargetType="Window">

            <!--#region 主体内容 -->
            <Border Padding="0,30,0,0">
                <Grid x:Name="RootGrid" Background="{TemplateBinding Background}">

                    <!--#region 标题栏 -->
                    <Border MouseDown="Border_MouseDown" Background="{DynamicResource MaterialDesignBackground}" VerticalAlignment="Top" Height="60" Margin="0,-29,140,0">
                        <DockPanel Margin="10,0">
                            <!--标题-->
                            <Button Style="{StaticResource MaterialDesignIconButton}" Margin="5,0,5,3" DockPanel.Dock="Left">
                                <Image x:Name="TitleIcon" Source="../Source/KitX.png" Margin="10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            </Button>

                            <TextBlock x:Name="TitleBox" Margin="5,0,5,3" DockPanel.Dock="Left" VerticalAlignment="Center" Foreground="{DynamicResource MaterialDesignToolForeground}" FontSize="16" Text="{DynamicResource TitleBar_Title}">
                                <TextBlock.Effect>
                                    <DropShadowEffect Direction="0" ShadowDepth="0" Color="White" BlurRadius="5"/>
                                </TextBlock.Effect>
                            </TextBlock>
                            
                            <Menu DockPanel.Dock="Left" IsMainMenu="True" VerticalAlignment="Center" Margin="5,0" Style="{StaticResource MaterialDesignMenu}" Background="{DynamicResource MaterialDesignBackground}">

                                <!--#region 文件菜单 -->
                                <MenuItem Header="{DynamicResource MainMenu_Files}">
                                    <MenuItem Header="{DynamicResource MainMenu_Files_Open}" Click="Adder_Click"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Files_Dispose}" Tag="uri:unload" Click="OpenLink"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Files_Export}">
                                        <MenuItem Header="{DynamicResource MainMenu_Files_Export_Config}" Tag="uri:developing" Click="OpenLink"/>
                                        <MenuItem Header="{DynamicResource MainMenu_Files_Export_Tools}" Tag="uri:developing" Click="OpenLink"/>
                                    </MenuItem>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Edit_Options}" Click="JumpTo_Cog"/>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Files_ReStart}" Command="hc:ControlCommands.ShutdownApp" Click="ReStart"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Files_Exit}" Command="hc:ControlCommands.ShutdownApp" Click="ExitApp"/>
                                </MenuItem>
                                <!--#endregion-->

                                <!--#region 编辑菜单 -->
                                <MenuItem Header="{DynamicResource MainMenu_Edit}" IsEnabled="{Binding ElementName=ToolsList, Path=SelectedIndex, Converter={StaticResource EditMenuEnableConverter}}">
                                    <MenuItem Header="{DynamicResource MainMenu_Edit_Copy}"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Edit_Cut}"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Edit_Paste}"/>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Edit_Redo}"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Edit_Undo}"/>
                                </MenuItem>
                                <!--#endregion-->

                                <!--#region 视图菜单 -->
                                <MenuItem Header="{DynamicResource MainMenu_View}">
                                    <MenuItem Header="{DynamicResource MainMenu_View_Lib}">
                                        <MenuItem Header="{DynamicResource MainMenu_View_Lib_Home}" Click="Normal_Jump" Tag="0,0"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Lib_Info}" Click="Normal_Jump" Tag="0,1"/>
                                    </MenuItem>
                                    <MenuItem Header="{DynamicResource MainMenu_View_Shop}" Click="Normal_Jump" Tag="1,0"/>
                                    <MenuItem Header="{DynamicResource MainMenu_View_Plan}" Click="Normal_Jump" Tag="2,0"/>
                                    <MenuItem Header="{DynamicResource MainMenu_View_Acc}" Click="JumpTo_User">
                                        <MenuItem Header="{DynamicResource MainMenu_View_Acc_SignIn}" Click="Normal_Jump" Tag="3,0" IsEnabled="{Binding ElementName=Acc_Pages, Path=SelectedIndex, Converter={StaticResource GetAccMenuEnable}}"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Acc_SignUp}" Click="Normal_Jump" Tag="3,1" IsEnabled="{Binding ElementName=Acc_Pages, Path=SelectedIndex, Converter={StaticResource GetAccMenuEnable}}"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Acc_Info}" Click="Normal_Jump" Tag="3,2" IsEnabled="{Binding ElementName=Acc_Pages, Path=SelectedIndex, Converter={StaticResource GetAccInfoMenuEnable}}"/>
                                    </MenuItem>
                                    <MenuItem Header="{DynamicResource MainMenu_View_Cog}">
                                        <MenuItem Header="{DynamicResource MainMenu_View_Cog_Environment}" Click="Normal_Jump" Tag="4,0"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Cog_Personalize}" Click="Normal_Jump" Tag="4,1"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Cog_Nature}" Click="Normal_Jump" Tag="4,2"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Cog_Deliver}" Click="Normal_Jump" Tag="4,3"/>
                                        <MenuItem Header="{DynamicResource MainMenu_View_Cog_Support}" Click="Normal_Jump" Tag="4,4"/>
                                    </MenuItem>
                                </MenuItem>
                                <!--#endregion-->

                                <!--#region 帮助菜单 -->
                                <MenuItem Header="{DynamicResource MainMenu_Help}">
                                    <MenuItem Header="{DynamicResource MainMenu_Help_View}" Tag="https://blog.catrol.cn/2021/05/05/%5BKitX%5DUsing/" Click="OpenLink"/>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_Log}" Tag="uri:logFile" Click="OpenLink"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_FeedBack}" Tag="uri:developing" Click="OpenLink">
                                        <MenuItem Header="{DynamicResource MainMenu_Help_FeedBack_Problem}" Tag="uri:developing" Click="OpenLink"/>
                                        <MenuItem Header="{DynamicResource MainMenu_Help_FeedBack_Suggestion}" Tag="uri:developing" Click="OpenLink"/>
                                        <MenuItem Header="{DynamicResource MainMenu_Help_FeedBack_Mine}" Tag="uri:developing" Click="OpenLink"/>
                                    </MenuItem>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_TechSupport}" Tag="uri:developing" Click="OpenLink"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_Tsk}" Tag="uri:developing" Click="OpenLink"/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_CheckUpdate}" Click="CheckForUpdate"/>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_KeyBoardHelper}" Tag="https://docs.catrol.cn/" Click="OpenLink"/>
                                    <Separator/>
                                    <MenuItem Header="{DynamicResource MainMenu_Help_About}" Tag="4,4" Click="ToAbout"/>
                                </MenuItem>
                                <!--#endregion-->

                            </Menu>
                            
                            <Grid DockPanel.Dock="Right">
                                <StackPanel Orientation="Horizontal">

                                    <!--#region 保持置顶 -->
                                    <ToggleButton x:Name="KeepToper" ToolTip="{DynamicResource TitleBar_Pin_ToolTip}" Checked="KeepToper_Checked" Unchecked="KeepToper_Unchecked" Width="35" Background="Transparent" Margin="10,0" Style="{StaticResource MaterialDesignActionToggleButton}">
