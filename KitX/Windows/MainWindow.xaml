                                        </materialDesign:ToggleButtonAssist.OnContent>
                                    </ToggleButton>
                                    <!--#endregion-->

                                    <!--#region 工具栏调整 -->
                                    <ToggleButton x:Name="AppsBarManager" ToolTip="{DynamicResource TitleBar_AppsBarManager_ToolTip}" Checked="AppsBarManager_Checked" Unchecked="AppsBarManager_Unchecked" Width="35" Background="Transparent" Margin="10,0" Style="{StaticResource MaterialDesignActionToggleButton}">
                                        <Path Width="20" Height="20" Fill="{DynamicResource LinearBlue}" Data="F1 M 20 0 L 20 20 L 0 20 L 0 0 Z M 18.75 1.25 L 12.5 1.25 L 12.5 6.25 L 18.75 6.25 Z M 12.5 12.5 L 18.75 12.5 L 18.75 7.5 L 12.5 7.5 Z M 1.25 1.25 L 1.25 12.5 L 11.25 12.5 L 11.25 1.25 Z M 1.25 18.75 L 7.5 18.75 L 7.5 13.75 L 1.25 13.75 Z M 18.75 18.75 L 18.75 13.75 L 8.75 13.75 L 8.75 18.75 Z "/>
                                        <materialDesign:ToggleButtonAssist.OnContent>
                                            <Path Width="20" Height="20" Fill="{DynamicResource LinearWarning}" Data="F1M20,0L20,20 0,20 0,0z M18.75,1.25L12.5,1.25 12.5,6.25 18.75,6.25z M12.5,12.5L18.75,12.5 18.75,7.5 12.5,7.5z M1.25,1.25L1.25,12.5 11.25,12.5 11.25,1.25z M1.25,18.75L7.5,18.75 7.5,13.75 1.25,13.75z M18.75,18.75L18.75,13.75 8.75,13.75 8.75,18.75z"/>
                                        </materialDesign:ToggleButtonAssist.OnContent>
                                        <ToggleButton.ContextMenu>
                                            <ContextMenu>
                                                <MenuItem Header="{DynamicResource TitleBar_AppsBarManager_ContextMenu_Close}" Click="CloseAppsbar"/>
                                            </ContextMenu>
                                        </ToggleButton.ContextMenu>
                                    </ToggleButton>
                                    <!--#endregion-->

                                    <!--#region 弹出菜单 -->
                                    <materialDesign:PopupBox Margin="10,0" materialDesign:ThemeAssist.Theme="Inherit"
                                                             Width="30" HorizontalAlignment="Center" Height="{Binding ElementName=TitleBar, Path=ActualHeight}"
                                                             PlacementMode="BottomAndAlignCentres">

                                        <!--#region 显示的内容 -->
